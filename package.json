{
  "name": "mda_next",
  "private": true,
  "version": "0.7.6",
  "author": "PoleStarGlobalDefense",
  "repository": {
    "type": "gitlab",
    "url": "https://gitlab.com/polestardefense/mda-next/mda-ui"
  },
  "main": "./src/main.tsx",
  "scripts": {
    "build": "webpack --progress --config webpack.config.ts --env --mode production",
    "build:dev": "webpack --progress --config webpack.config.ts --env --mode development",
    "clean": "rm -rf ./dist",
    "rebuild": "rm -rf node_modules && rm -rf packge-lock.json && npm cache clean --force && npm install",
    "dev": "webpack serve --progress --color --config webpack.config.ts --mode development --port 5173",
    "watch": "webpack serve --config webpack.config.ts --watch --progress --color --port 5173 --open",
    "format": "prettier --check ./src",
    "format:check": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test:debug": "npx playwright test --config ./playwright.config.ts",
    "test:e2e": "npx playwright test",
    "test:e2e:chrome": "npx playwright test --project=Chrome",
    "test:e2e:ui": "npx playwright test --ui --project=Chrome",
    "test:e2e:smoke": "npx playwright test --grep @Smoke --ui --project=Chrome",
    "test:report": "allure serve"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/lato": "^5.0.8",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.5",
    "@mui/x-data-grid": "^6.11.1",
    "axios": "^1.4.0",
    "cesium": "^1.108.0",
    "jotai": "^2.3.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^6.15.0",
    "react-use-websocket": "^4.3.1",
    "resium": "^1.17.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@playwright/test": "^1.37.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/node": "^20.5.0",
    "@typescript-eslint/parser": "^6.3.0",
    "adm-zip": "^0.5.10",
    "allure-commandline": "^2.23.1",
    "allure-playwright": "^2.5.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.47.0",
    "html-webpack-plugin": "^5.5.3",
    "html-webpack-tags-plugin": "^3.0.2",
    "jotai-devtools": "^0.6.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.27",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.1",
    "prettier": "3.0.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.65.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "winston": "^3.10.0"
  },
  "overrides": {
    "yaml": "2.3.1"
  }
}
